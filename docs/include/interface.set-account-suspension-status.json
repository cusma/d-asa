      "type": "address",
      "name": "holding_address",
      "desc": "Account Holding Address"
    },
    {
      "type": "bool",
      "name": "suspended",
      "desc": "Suspension status"
    }
  ],
  "returns": {
    "type": "uint64",
    "desc": "Timestamp of the set account suspension status"
  },
  "errors": [
    {
      "code": "UNAUTHORIZED",
